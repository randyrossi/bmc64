// template of dialog for all languages

IDD_EASYFLASH_SETTINGS_DIALOG DIALOG DISCARDABLE  0, 0, 180, 110
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
FONT 8, "MS Sans Serif"
BEGIN
    AUTOCHECKBOX    "", IDC_EASYFLASH_JUMPER,
                    7, 7, 99, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    AUTOCHECKBOX    "", IDC_EASYFLASH_CRT_OPTIMIZE,
                    7, 24, 99, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    AUTOCHECKBOX    "", IDC_EASYFLASH_AUTOSAVE,
                    7, 41, 99, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    PUSHBUTTON      "", IDC_EASYFLASH_SAVE_NOW,
                    7, 58, 99, 14, WS_TABSTOP

    DEFPUSHBUTTON   "", IDOK, 30,90,50,14, WS_TABSTOP
    PUSHBUTTON      "", IDCANCEL, 90,90,50,14, WS_TABSTOP
END

